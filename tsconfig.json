{
    "compilerOptions": {
        "module": "commonjs",
        "rootDir": ".",
        "outDir": ".fedog-cache/a"
    },
    "filesGlob": [
        "./**/*.ts"
    ],
    "exclude": [
        "node_modules",
        ".fedog-cache",
        ".git",
        ".sass-cache"
    ],
    "files": [
        "./static/htmls/frame/frame.ts",
        "./static/htmls/login/login.ts",
        "./static/htmls/register/register.ts",
        "./static/scripts/libs/small-spa/_loader.ts",
        "./static/scripts/libs/small-spa/_small-spa-conf.ts",
        "./static/scripts/libs/small-spa/small-spa.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
